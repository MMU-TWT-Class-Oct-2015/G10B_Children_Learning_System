CREATE DATABASE CLS;

CREATE TABLE PROFILE
(
	USER_ID INT AUTO_INCREMENT PRIMARY KEY,
	USER_NAME VARCHAR(20) NOT NULL UNIQUE,
	PASSWORD VARCHAR(40) NOT NULL,
	EMAIL VARCHAR(40) NOT NULL,
	ADD_HS INT,
	SUB_HS INT,
	MUL_HS INT,
	DIV_HS INT,
	TOTAL_HS INT
);

ALTER TABLE PROFILE AUTO_INCREMENT = 10001;


